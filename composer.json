{
    "name": "keithbrink/amazon-mws-laravel",
    "description": "A Laravel library for Amazon's MWS web services, supporting Laravel 5-7+",
    "license": "Apache-2.0",
    "keywords": [
        "API",
        "Amazon",
        "PHP",
        "MWS",
        "Laravel"
    ],
    "authors": [
        {
            "name": "Keith Brink",
            "email": "keith.brink@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "symfony/http-kernel": "^5.0|^6.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "illuminate/http": "^9.0",
        "illuminate/routing": "^9.0",
        "illuminate/view": "^9.0",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "KeithBrink\\AmazonMws\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KeithBrink\\AmazonMws\\MwsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}