{
    "name": "keithbrink/amazon-mws-laravel",
    "description": "Use Amazon's MWS web services with Laravel 5.x. Based on creacoon/amazon-mws-laravel package and modified to make it compatible with latest Laravel releases (+ bugfixes).",
    "license": "Apache-2.0",
    "keywords": ["API", "Amazon", "PHP", "MWS", "Laravel"],
    "authors": [
        {
            "name": "Keith Brink",
            "email": "keith.brink@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-curl": "*",
        "symfony/http-kernel": "~2.7|~3.0|~4.0",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "illuminate/http": "~5.3",
        "illuminate/routing": "~5.3",
        "illuminate/view": "~5.3",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.0",
        "vlucas/phpdotenv": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "KeithBrink\\AmazonMws\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KeithBrink\\AmazonMws\\MwsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
