{
    "name": "martinperiphore/amazon-mws-laravel",
    "description": "A Laravel library for Amazon's MWS web services, supporting Laravel 5-7+",
    "license": "Apache-2.0",
    "keywords": ["API", "Amazon", "PHP", "MWS", "Laravel"],
    "authors": [
        {
            "name": "Martin Ray Gales",
            "email": "martinperiphore@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "symfony/http-kernel": "^5.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "illuminate/http": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/view": "^8.0",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Martinperiphore\\AmazonMws\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Martinperiphore\\AmazonMws\\MwsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
